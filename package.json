{
  "name": "Todos",
  "version": "1.0.0",
  "description": "React Native app powered by Baker",
  "author": "",
  "license": "ISC",
  "scripts": {
    "setup": "node ./cli/baker.js generate:app",
    "generate": "node ./cli/baker.js generate",
    "ios": "node node_modules/react-native/local-cli/cli.js run-ios",
    "android": "node node_modules/react-native/local-cli/cli.js run-android",
    "build:ios": "node ./node_modules/react-native/local-cli/cli.js bundle --platform ios --entry-file index.ios.js --bundle-output ios/Todos/main.jsbundle --dev=false --minify --verbose",
    "mongo": "node ./node_modules/mongodb-runner/bin/mongodb-runner start --name=dev --dbpath ./.mongo --purge false",
    "server": "npm run mongo && NODE_ENV=development babel-watch ./server --presets es2015 --plugins transform-object-rest-spread --watch ./server/**/*.js",
    "server-without-mongo-runner": "NODE_ENV=development babel-watch ./server --presets es2015 --plugins transform-object-rest-spread --watch ./server/**/*.js",
    "server-debug": "npm run mongo && NODE_ENV=development babel-node --debug -- ./server"
  },
  "engines": {
    "node": ">=4.3"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "express": "^4.13.4",
    "express-graphql": "^0.5.3",
    "graphql": "^0.6.0",
    "immutable": "^3.8.1",
    "lokka": "^1.7.0",
    "parse": "^1.8.5",
    "parse-dashboard": "^1.0.13",
    "parse-server": "^2.2.11",
    "react": "^15.1.0",
    "react-native": "^0.27.0",
    "react-native-swipeout": "git+https://github.com/magrinj/react-native-swipeout.git#403d973504b58ede25c137fb31b0eff7e03b0a66",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-immutable": "^3.0.6",
    "redux-saga": "^0.10.5",
    "reselect": "^2.4.0"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.2",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-watch": "^2.0.2",
    "eslint": "^2.8.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^4.3.0",
    "generator-rn": "^0.1.2",
    "minimist": "^1.2.0",
    "mongodb-runner": "^3.3.2",
    "remote-redux-devtools": "^0.1.6",
    "yeoman-environment": "^1.6.1",
    "yeoman-generator": "^0.21.2"
  }
}
